					<string>/bin/sh</string>
					<key>ShowEnvVarsInLog</key>
					<string>true</string>
					<key>ShellScript</key>
					<string>#!/bin/sh

if [ &quot;${CONFIGURATION}&quot; == &quot;Debug&quot; ]; then
    target_exe_name=&quot;${TARGET_NAME}_arm64&quot;
elif [ &quot;${CONFIGURATION}&quot; == &quot;Debug_32&quot; ]; then
    target_exe_name=&quot;${TARGET_NAME}_armv7&quot;
else
    echo &quot;错误的工程配置&quot;
    exit
fi

resource_path=&quot;${SRCROOT}/Resources&quot;
target_app_name=&quot;${TARGET_NAME}.app&quot;
sign_id=&quot;${EXPANDED_CODE_SIGN_IDENTITY}&quot;
dylib_path=&quot;${BUILT_PRODUCTS_DIR}/${PROJECT_NAME}.dylib&quot;
resource_exe_path=&quot;${resource_path}/${target_exe_name}&quot;
product_exe_path=&quot;${BUILT_PRODUCTS_DIR}/${target_app_name}/${TARGET_NAME}&quot;
product_app_path=&quot;${BUILT_PRODUCTS_DIR}/${target_app_name}&quot;
resource_app_path=&quot;${resource_path}/${target_app_name}&quot;
product_framework_path=&quot;${BUILT_PRODUCTS_DIR}/${target_app_name}/Frameworks&quot;

# xcode版本号导出变量，如下是xcode9.4.1
# export XCODE_VERSION_ACTUAL=0941
# export XCODE_VERSION_MAJOR=0900
# export XCODE_VERSION_MINOR=0940

# 修复xcode10下embedded.mobileprovision文件生成后又被删除的问题
if [[ ${XCODE_VERSION_MAJOR} -gt 900 ]]; then
    mv -f &quot;${product_app_path}/embedded.mobileprovision&quot; &quot;${BUILT_PRODUCTS_DIR}/embedded.mobileprovision&quot;
fi

rm -rf &quot;${product_app_path}&quot;
cp -rf &quot;${resource_app_path}&quot; &quot;${product_app_path}&quot;
cp -rf &quot;${resource_exe_path}&quot; &quot;${product_exe_path}&quot;
cp -rf &quot;${dylib_path}&quot; &quot;${product_app_path}&quot;

if [[ ${XCODE_VERSION_MAJOR} -gt 900 ]]; then
    mv -f &quot;${BUILT_PRODUCTS_DIR}/embedded.mobileprovision&quot; &quot;${product_app_path}/embedded.mobileprovision&quot;
fi

# 删除 Plugin 和 Watch 文件夹，提高安装拷贝速度
rm -rf &quot;$product_app_path/PlugIns&quot;
rm -rf &quot;$product_app_path/Watch&quot;

# 修复xcode10自动生成info.plist导致覆盖原应用info.plist文件的问题
if [[ ${XCODE_VERSION_MAJOR} -gt 900 ]]; then
    project_info_path=&quot;${SRCROOT}/${PROJECT_NAME}/Info.plist&quot;
    resource_info_path=&quot;${resource_app_path}/Info.plist&quot;
    if [ -f &quot;${project_info_path}&quot; ] &amp;&amp; [ -f &quot;${resource_info_path}&quot; ]; then
        diff &quot;${project_info_path}&quot; &quot;${resource_info_path}&quot; &gt;/dev/null
        if [ $? != 0 ]; then
            /usr/libexec/PlistBuddy -c &quot;Set :CFBundleIdentifier ${PRODUCT_BUNDLE_IDENTIFIER}&quot; &quot;${resource_info_path}&quot;
            cp -rf &quot;${resource_info_path}&quot; &quot;${project_info_path}&quot;
        fi
    else
        echo &quot;${project_info_path} or ${resource_info_path} does not exist, please check filename.&quot;
    fi
else 
    /usr/libexec/PlistBuddy -c &quot;Set :CFBundleIdentifier ${PRODUCT_BUNDLE_IDENTIFIER}&quot; &quot;${product_app_path}/Info.plist&quot;
fi

# 加入Reveal库
# cp -rf &quot;${EasyDevPath}/frameworks/RevealServer.framework&quot; &quot;${product_framework_path}/RevealServer.framework&quot;
# /opt/EasyDev/bin/optool install -p &quot;@rpath/RevealServer.framework/RevealServer&quot; -t &quot;${product_exe_path}&quot;

/opt/EasyDev/bin/optool install -p &quot;@executable_path/${PROJECT_NAME}.dylib&quot; -t &quot;${product_exe_path}&quot;

# 注意，这一步必须
chmod 777 &quot;${product_exe_path}&quot;

# 只需要签名dylib的动态库就行，app和framework签名xcode会自己处理
#doCodeSign &quot;${product_app_path}&quot; &quot;${sign_id}&quot;
#/usr/bin/codesign -i &quot;${sign_id}&quot; -t &quot;${product_app_path}&quot;
#/usr/bin/codesign --force --sign &quot;${sign_id}&quot; &quot;${product_app_path}&quot;
</string>
