			<key>Description</key>
			<string>Target App Name.</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>AppStore</string>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>TargetAppBundleId</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Target App Bundle Id:</string>
			<key>Description</key>
			<string>Target App Bundle Id.</string>
			<key>Type</key>
			<string>text</string>
			<key>Placeholder</key>
			<string>com.apple.appStore</string>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>___PACKAGENAME___.m</string>
		<string>PrefixHeader.pch</string>
		<string>Info.plist</string>
		<string>../Resources/___VARIABLE_TargetAppName____armv7</string>
		<string>../Resources/___VARIABLE_TargetAppName____arm64</string>
		<string>../Resources/___VARIABLE_TargetAppName___.app</string>
	</array>
	<key>Definitions</key>
	<dict>
		<key>___PACKAGENAME___.m</key>
		<dict>
			<key>Path</key>
			<string>___PACKAGENAME___.m</string>
		</dict>
		<key>PrefixHeader.pch</key>
		<dict>
			<key>Group</key>
			<array>
				<string>Supporting Files</string>
			</array>
			<key>Path</key>
			<string>PrefixHeader.pch</string>
		</dict>
		<key>Info.plist</key>
		<dict>
			<key>Group</key>
			<array>
				<string>Supporting Files</string>
			</array>
			<key>Path</key>
			<string>Info.plist</string>
		</dict>
		<key>../Resources/___VARIABLE_TargetAppName____armv7</key>
		<dict>
			<key>Beginning</key>
			<string>This is a armv7 bin file</string>
			<key>Group</key>
			<array>
				<string>Resources</string>
			</array>
		</dict>
		<key>../Resources/___VARIABLE_TargetAppName____arm64</key>
		<dict>
			<key>Beginning</key>
			<string>This is a arm64 bin file</string>
			<key>Group</key>
			<array>
				<string>Resources</string>
			</array>
		</dict>
